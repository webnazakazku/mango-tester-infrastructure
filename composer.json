{
	"name": "webnazakazku/mango-tester-infrastructure",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": ">=8.1",
		"nette/bootstrap": "^3.2",
		"nette/di": "~3.2",
		"nette/tester": "^2.3.1",
		"nette/utils": "^4.0"
	},
	"require-dev": {
		"mockery/mockery": "^1.0",
		"contributte/qa": "^0.3",
		"phpstan/phpstan": "^1.0",
		"phpstan/phpstan-deprecation-rules": "^1.0",
		"phpstan/phpstan-nette": "^1.0",
		"tracy/tracy": "~2.10"
	},
	"autoload": {
		"classmap": [
			"src"
		]
	},
	"autoload-dev": {
		"classmap": [
			"tests/src"
		]
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
